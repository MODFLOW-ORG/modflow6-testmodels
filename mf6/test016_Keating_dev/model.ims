begin options
  print_option all
  #dev_no_ptc
  dev_ptc_del0 12500.
  dev_ptc_output fileout ptc.output.out
end options

begin nonlinear
	outer_dvclose 1.e-4
	outer_maximum 2000 
	under_relaxation dbd
	under_relaxation_theta 0.70
	under_relaxation_kappa 0.08
	under_relaxation_gamma 0.05
	under_relaxation_momentum 0.
	backtracking_number 20
	backtracking_tolerance 2.
	backtracking_reduction_factor 0.2
	backtracking_residual_limit 5.000000e-04
end nonlinear


begin linear
	inner_dvclose 1.0e-5
	inner_rclose 1.0e-4 relative_rclose
	inner_maximum 100
	linear_acceleration bicgstab
	scaling_method none
	reordering_method none
	relaxation_factor 0.0
	number_orthogonalizations 2
	preconditioner_levels 8
	preconditioner_drop_tolerance 0.
end linear
