begin options
  print_option all
end options

begin nonlinear
	outer_dvclose 1.e-5
	outer_maximum 500 
	under_relaxation none
	under_relaxation_theta 0.9
	under_relaxation_kappa 0.100000e-03
	under_relaxation_gamma 0.
	under_relaxation_momentum 0.1
