BEGIN options
  BUDGET  FILEOUT  gwt_elder.cbc
  CONCENTRATION  FILEOUT  gwt_elder.ucn
  CONCENTRATION  PRINT_FORMAT  COLUMNS  10  WIDTH  15  DIGITS  6  GENERAL
END options

BEGIN period  1
  SAVE  CONCENTRATION  LAST
  SAVE  BUDGET  LAST
  PRINT  CONCENTRATION  FIRST
  PRINT  BUDGET  LAST
END period  1

BEGIN period  2
  SAVE  CONCENTRATION  LAST
END period  2

BEGIN period  200
  SAVE  CONCENTRATION  LAST
  SAVE  BUDGET  LAST
  PRINT  CONCENTRATION  LAST
  PRINT  BUDGET  LAST
END period  200

